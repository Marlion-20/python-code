# filer(function, iterable)
friends = [("amy",21),
           ("juan",12),
           ("leila",33),
           ("maya",18),
           ("kisya",17)]
age = lambda data: data[1] >=18
drinking_buddies = list(filter(age,friends))
for i in drinking_buddies:
    print(i)

